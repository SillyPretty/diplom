{
  "trailingComma": "none",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "arrowParens": "avoid",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "/routes/",
    "/hooks/",
    "/screens/",
    "/components/",
    "/ui/",
    "^../(.*)",
    "^./(.*)",
    "/module.scss/"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
